{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "andrew/filament-blog": "dev-main",
        "anhskohbo/no-captcha": "^3.6",
        "austintoddj/canvas": "^6.0",
        "based/momentum-modal": "^0.4.0",
        "bezhansalleh/filament-shield": "^3.3",
        "cknow/laravel-money": "^8.1",
        "cms-multi/filament-clear-cache": "^2.0",
        "cviebrock/eloquent-sluggable": "^11.0",
        "cybercog/laravel-love": "^10.0",
        "fakerphp/faker": "^1.24",
        "filament/filament": "^3.2",
        "filament/spatie-laravel-media-library-plugin": "*",
        "inertiajs/inertia-laravel": "2.x-dev",
        "jenssegers/agent": "^2.6",
        "joaopaulolndev/filament-general-settings": "^1.0",
        "lara-zeus/sky": "^3.4",
        "laravel/cashier": "^15.4",
        "laravel/framework": "^11.31",
        "laravel/horizon": "^5.29",
        "laravel/octane": "^2.6",
        "laravel/sanctum": "^4.0",
        "laravel/scout": "^10.11",
        "laravel/tinker": "^2.9",
        "league/flysystem-aws-s3-v3": "^3.29",
        "mavinoo/laravel-batch": "^2.4",
        "nnjeim/world": "^1.1",
        "opcodesio/log-viewer": "^3.11",
        "overtrue/laravel-favorite": "^5.2",
        "pelmered/filament-money-field": "^1.4",
        "php-ffmpeg/php-ffmpeg": "^1.3",
        "spatie/eloquent-sortable": "^4.4",
        "spatie/enum": "^3.13",
        "spatie/laravel-data": "^4.11",
        "spatie/laravel-medialibrary": "^11.9",
        "spatie/laravel-permission": "^6.10",
        "spatie/laravel-query-builder": "^6.2",
        "spatie/laravel-sitemap": "^7.3",
        "spatie/laravel-typescript-transformer": "^2.5",
        "spatie/schema-org": "^3.23",
        "spiral/roadrunner-cli": "^2.6.0",
        "spiral/roadrunner-http": "^3.3.0",
        "staudenmeir/laravel-adjacency-list": "^1.0",
        "stripe/stripe-php": "^13.18",
        "vinkla/hashids": "^12.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.14",
        "laravel/breeze": "^2.2",
        "laravel/dusk": "^8.2",
        "laravel/pail": "^1.1",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.39",
        "laravel/ui": "^4.6",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "pestphp/pest": "^3.6",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "autoload": {
        "files": [
            "app/Helpers/global-helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "repositories": [
    {
        "type": "vcs",
        "url": "git@github.com:andrewperoramas/filament-blog.git"

    }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}
